# Admin RFID Tag UIDs (comma-separated)
# Example: ADMIN_TAG_UIDS=04A224B1C8,04B335C2D9
ADMIN_TAG_UIDS=2659423e,ab9c423e

# Server Port
PORT=3000

# CouchDB Configuration
COUCHDB_URL=http://localhost:5984
COUCHDB_USER=admin
COUCHDB_PASSWORD=password
COUCHDB_DB_NAME=mks_members

# Seed default data (areas/equipment/members)
SEED_DATA=false

# Seed mode: default | mass
SEED_MODE=default

# Mass seed counts (used when SEED_MODE=mass)
SEED_MEMBERS=150
SEED_AREAS=20
SEED_EQUIPMENT=200
